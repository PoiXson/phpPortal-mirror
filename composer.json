{
	"name":        "pxn/phpportal",
	"description": "Website Utilities Library",
	"type":        "library",
	"homepage":    "https://poixson.com",
	"license":     "GPL-3.0",
	"keywords": [
		"framework", "utilities"
	],
	"support": {
		"source": "https://git.poixson.com/summary/pxn!phpPortal.git",
		"issues": "https://git.poixson.com/tickets/pxn!phpPortal.git",
		"irc":    "irc://irc.esper.net/poixson",
		"discord": "https://discord.gg/jHgP2a3kDx"
	},
	"authors": [{
		"name": "lorenzop",
		"role": "Developer"
	}],
	"autoload": {
		"psr-4": {
			"pxn\\phpPortal\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"pxn\\phpPortal\\tests\\": "tests/"
		}
	},
	"archive": {
		"exclude": [
			"tests/",
			"examples/"
		]
	},
	"require": {
		"pxn/phputils": "*",
		"pxn/webresources": "*",
		"twig/twig": "*",
		"league/commonmark": "*",
		"pxn/composerlocaldev": "*"
	},
	"require-dev": {
		"filp/whoops": "*",
		"kint-php/kint": "*",
		"phpunit/phpunit": "*"
	}
}
